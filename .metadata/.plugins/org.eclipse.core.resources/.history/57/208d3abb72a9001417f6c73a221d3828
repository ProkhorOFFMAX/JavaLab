package ru.spbstu.telematics.Prokhorov.lab2;

public class MainExample {

	public static void main(String[] args) {
		MyArrayQueue<Integer>Q =new MyArrayQueue<Integer>();//создание
		System.out.print("Размер очереди на данный момент = "+ Q.size()+ "\n");
		Q.add(1);
		System.out.print("Размер очереди после добавления = "+ Q.size()+ "\n");
		Q.add(2);
		System.out.print("Размер очереди после добавления = "+ Q.size()+ "\n");
		System.out.print(Q.get()+ " ");
		System.out.print(Q.get()+ " ");
		System.out.print(Q.get()+ " ");
		Q.add(9);
		System.out.print(Q.get()+ "\n ");
		Q.rotate(1);
		for(int i=0; i<8; i++)
			Q.add(i);
		for(int i=0; i<8; i++)
			System.out.print(Q.get()+ " ");
	}
}
